<div class="row" id="nav-tabs">
	<div class="col-xs-3 col-sm-3 col-md-3">
		<% if (userInfo[0].profile_img) { %>
			<% if (userInfo[0].profile_img_status == 6) { %>
				<img class="nav-profile-img content-page" id="rotate" src="/images/profile-imgs/<%= userInfo[0].profile_img %>" alt="profile image">
			<% } else { %>
				<img class="nav-profile-img content-page" src="/images/profile-imgs/<%= userInfo[0].profile_img %>" alt="profile image">
			<% } %>
		<% } else { %>
			<img class="nav-profile-img content-page" src="/images/profile-imgs/default-profile.jpg %>" alt="profile image">
		<% } %>
	</div>
	<div class="col-xs-7 col-sm-8 col-md-8">
		<div id="tabs">
			<a href="/onlineoffice/dashboard">
				<div class="smaller-circle">
					<i class="fa fa-pencil-square-o fa-2x" aria-hidden="true"></i>
					<div>Dashboard</div>
				</div>
			</a>
			<a href="/onlineoffice/organization">
				<div class="smaller-circle">
					<i class="fa fa-handshake-o fa-2x" aria-hidden="true"></i>
					<div>Organization</div>
				</div>
			</a>
			<a href="/onlineoffice/contacts">
				<div class="smaller-circle">
					<i class="fa fa-address-book-o fa-2x" aria-hidden="true"></i>
					<div>Contacts</div>
				</div>
			</a>
			<a href="/onlineoffice/calendar/<%= currentUser.id %>">
				<div class="smaller-circle">
					<i class="fa fa-calendar-check-o fa-2x" aria-hidden="true"></i>
					<div>Calendar</div>
				</div>
			</a>
			<a href="/onlineoffice/communitytasks/<%= currentUser.id %>">
				<div class="smaller-circle">
					<i class="fa fa-calendar-check-o fa-2x" aria-hidden="true"></i>
					<div>Task Grid</div>
				</div>
			</a>
			<a href="/onlineoffice/projects">
				<div class="smaller-circle">
					<i class="fa fa-paper-plane-o fa-2x" aria-hidden="true"></i>
					<div>Projects</div>
				</div>
			</a>
			<a href="/onlineoffice/storage/<%= currentUser.id %>">
				<div class="smaller-circle">
					<i class="fa fa-archive fa-2x" aria-hidden="true"></i>
					<div>Storage</div>
				</div>
			</a>
			<a href="/onlineoffice/voicemail/<%= currentUser.id %>">
				<div class="smaller-circle">
					<i class="fa fa-video-camera fa-2x" aria-hidden="true"></i>
					<div>Videomail</div>
				</div>
			</a>
			<a href="/onlineoffice/inbox">
				<div class="smaller-circle">
					<i class="fa fa-envelope-o fa-2x" aria-hidden="true"></i>
					<div>Inbox</div>
				</div>
			</a>
			<a href="/onlineoffice/whiteboard">
				<div class="smaller-circle">
					<i class="fa fa-pencil-square-o fa-2x" aria-hidden="true"></i>
					<div>Whiteboard</div>
				</div>
			</a>
			<a href="/onlineoffice/settings">
				<div class="smaller-circle">
					<i class="fa fa-cog fa-2x" aria-hidden="true"></i>
					<div>Settings</div>
				</div>
			</a>
		</div>
	</div>
	<div class="col-xs-2 col-sm-1 col-md-1 back-to-profile-main">
		<a href="/onlineoffice/home"><div id="close-sub-profiles"></div></a>
	</div class="smaller-circle">
</div>